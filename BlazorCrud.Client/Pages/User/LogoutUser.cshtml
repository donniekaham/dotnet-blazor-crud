@page "/user/logout"
@inject HttpClient Http
@inject Microsoft.AspNetCore.Blazor.Services.IUriHelper UriHelper
@inject LocalStorage Storage

@functions {

    protected override void OnInit()
    {
        Storage["JWT-Token"] = String.Empty;
        Storage["JWT-Time-Exire"] = String.Empty;
        // Storage["App-State"] = "logged-out";
        Http.DefaultRequestHeaders.Authorization = new AuthenticationHeaderValue("Bearer", String.Empty);
        UriHelper.NavigateTo("/logout");
    }
}